    "text": "\u4FBF\u5A36\u4E86",
    "pn": "\u8DEF\u5F97",
    "text": "\u8DEF\u5F97",
    "text": "\u4E3A\u59BB\uFF0C\u4E0E\u5979\u540C\u623F\u3002\u8036\u548C\u534E\u4F7F\u5979\u6000\u5B55\u751F\u4E86\u4E00\u4E2A\u513F\u5B50\u3002 ",
    "v": "14",
    "text": "\u5987\u5973\u4EEC\u5BF9",
    "pn": "\u62FF\u4FC4\u7C73",
    "text": "\u62FF\u4FC4\u7C73",
    "text": "\u8BF4\uFF1A\u201C\u8036\u548C\u534E\u5F53\u53D7\u79F0\u9882\uFF01\u56E0\u4E3A\u4ECA\u65E5\u7942\u8D50\u7ED9\u4F60\u4E00\u4F4D\u81F3\u4EB2\uFF0C\u4F7F\u4F60\u540E\u7EE7\u6709\u4EBA\uFF0C\u613F\u8FD9\u5B69\u5B50\u5728",
    "pn": "\u4EE5\u8272\u5217",
    "text": "\u4EE5\u8272\u5217",
    "text": "\u5F97\u4EAB\u76DB\u540D\uFF01 ",
    "v": "15",
    "text": "\u4ED6\u5FC5\u8BA9\u4F60\u7684\u751F\u547D\u91CD\u65B0\u5F97\u529B\uFF0C\u5949\u517B\u4F60\uFF0C\u4F7F\u4F60\u5B89\u5EA6\u665A\u5E74\uFF0C\u56E0\u4E3A\u4ED6\u662F\u7231\u4F60\u7684\u513F\u5AB3\u5987\u6240\u751F\u7684\u3002\u6709\u8FD9\u513F\u5AB3\u5987\u6BD4\u6709\u4E03\u4E2A\u513F\u5B50\u8FD8\u8981\u597D\uFF01\u201D ",
    "v": "16",
    "pn": "\u62FF\u4FC4\u7C73",
    "text": "\u62FF\u4FC4\u7C73",
    "text": "\u63A5\u8FC7\u5A74\u5B69\u62B1\u5728\u6000\u4E2D\uFF0C\u7167\u987E\u4ED6\u3002 ",
    "v": "17",
    "text": "\u90BB\u5C45\u7684\u5987\u5973\u4EEC\u8BF4\uFF1A\u201C",
    "pn": "\u62FF\u4FC4\u7C73",
