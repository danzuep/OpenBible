    "text": "\n    ",
    "v": "9",
    "text": "\u6211\u7684\u59B9\u59B9\uFF0C\u6211\u7684\u65B0\u5A18\u554A\uFF0C",
    "style": "q1",
    "text": "\u4F60\u593A\u53BB\u4E86\u6211\u7684\u5FC3\u3002",
    "style": "q1",
    "text": "\u4F60\u7684\u79CB\u6CE2\u4E00\u95EA\uFF0C",
    "style": "q1",
    "text": "\u4F60\u9879\u94FE\u4E0A\u7684\u660E\u73E0\uFF0C",
    "style": "q1",
    "text": "\u593A\u53BB\u4E86\u6211\u7684\u5FC3\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "10",
    "text": "\u6211\u7684\u59B9\u59B9\uFF0C\u6211\u7684\u65B0\u5A18\u554A\uFF0C",
    "style": "q1",
    "text": "\u4F60\u7684\u7231\u60C5\u591A\u4E48\u8FF7\u4EBA\uFF0C",
    "style": "q1",
    "text": "\u4F60\u7684\u7231\u60C5\u6BD4\u7F8E\u9152\u9999\u751C\u3002",
    "style": "q1",
    "text": "\u4F60\u8EAB\u4E0A\u818F\u6CB9\u7684\u82AC\u82B3\u8FDC\u80DC\u8FC7\u4E00\u5207\u9999\u6599\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "11",
    "text": "\u6211\u7684\u65B0\u5A18\uFF0C\u4F60\u7684\u5634\u5507\u6EF4\u871C\uFF0C",
    "style": "q1",
    "text": "\u4F60\u7684\u820C\u5934\u5E95\u4E0B\u85CF\u7740\u5976\u4E0E\u871C\u3002",
    "style": "q1",
    "text": "\u4F60\u8863\u670D\u6563\u53D1\u7684\u9999\u6C14\uFF0C",
    "style": "q1",
    "text": "\u5C31\u50CF",
    "pn": "\u9ECE\u5DF4\u5AE9",
    "text": "\u9ECE\u5DF4\u5AE9",
    "text": "\u5C71\u4E00\u6837\u82AC\u82B3\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "12",
    "text": "\u6211\u7684\u59B9\u59B9\uFF0C\u6211\u7684\u65B0\u5A18\uFF0C",
    "style": "q1",
    "text": "\u4F60\u662F\u7D27\u9501\u7684\u56ED\u5B50\uFF0C",
    "style": "q1",
    "text": "\u5C01\u95ED\u7684\u4E95\uFF0C\u9690\u85CF\u7684\u6CC9\u6E90\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "13",
    "text": "\u4F60\u597D\u50CF\u4E00\u4E2A\u77F3\u69B4\u679C\u56ED\uFF0C",
    "style": "q1",
    "text": "\u7ED3\u6EE1\u4E86\u4E0A\u597D\u7684\u679C\u5B50\uFF0C",
    "style": "q1",
    "text": "\u8FD8\u6709\u51E4\u4ED9\u82B1\u548C\u54EA\u54D2\u6811\u3001",
    "style": "q1",
    "text": "\n    ",
    "v": "14",
    "text": "\u54EA\u54D2\u548C\u756A\u7EA2\u82B1\u3001\u83D6\u84B2\u548C\u8089\u6842\uFF0C",
    "style": "q1",
    "text": "\u4EE5\u53CA\u5404\u6837\u4E73\u9999\u6728\u3001\u6CA1\u836F\u3001\u6C89\u9999\u548C\u4E00\u5207\u4E0A\u597D\u7684\u9999\u6599\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "15",
    "text": "\u4F60\u662F\u56ED\u4E2D\u7684\u7518\u6CC9\uFF0C\u662F\u6D3B\u6C34\u7684\u4E95\uFF0C",
    "style": "q1",
    "text": "\u662F\u4ECE",
    "pn": "\u9ECE\u5DF4\u5AE9",
    "text": "\u9ECE\u5DF4\u5AE9",
    "text": "\u6D41\u4E0B\u6765\u7684\u6EAA\u6C34\u3002",
    "style": "sp",
    "text": "\u5973\u5B50\uFF1A",
    "style": "q1",
    "text": "\n    ",
    "v": "16",
    "text": "\u5317\u98CE\u554A\uFF0C\u5174\u8D77\u5427\uFF01",
    "style": "q1",
    "text": "\u5357\u98CE\u554A\uFF0C\u5439\u6765\u5427\uFF01",
    "style": "q1",
    "text": "\u5439\u5411\u6211\u7684\u56ED\u5B50\uFF0C",
    "style": "q1",
    "text": "\u4F7F\u5B83\u7684\u6E05\u9999\u56DB\u6EA2\u3002",
    "style": "q1",
    "text": "\u613F\u6211\u7684\u826F\u4EBA\u8D70\u8FDB\u81EA\u5DF1\u7684\u56ED\u4E2D\uFF0C",
    "style": "q1",
    "text": "\u4EAB\u53D7\u5176\u4E2D\u7684\u73CD\u679C\u3002",
    "c": "5",
    "style": "sp",
    "text": "\u7537\u5B50\uFF1A",
    "style": "q1",
    "text": "\n    ",
    "v": "1",
    "text": "\u6211\u7684\u59B9\u59B9\uFF0C\u6211\u7684\u65B0\u5A18\u554A\uFF0C",
    "style": "q1",
    "text": "\u6211\u5DF2\u7ECF\u6765\u5230\u81EA\u5DF1\u7684\u56ED\u4E2D\uFF0C",
    "style": "q1",
    "text": "\u91C7\u4E86\u6211\u7684\u6CA1\u836F\u548C\u9999\u6599\uFF0C",
    "style": "q1",
    "text": "\u54C1\u5C1D\u4E86\u6211\u7684\u8702\u623F\u548C\u8702\u871C\uFF0C",
    "style": "q1",
    "text": "\u4E5F\u4EAB\u7528\u4E86\u6211\u7684\u7F8E\u9152\u548C\u9C9C\u5976\u3002",
    "style": "sp",
    "text": "\u8036\u8DEF\u6492\u51B7\u7684\u5C11\u5973\uFF1A",
    "style": "q1",
    "text": "\u670B\u53CB\u4EEC\uFF0C\u5C3D\u60C5\u5403\u559D\u5427\u3002",
    "style": "q1",
    "text": "\u76F8\u7231\u7684\u4EBA\u554A\uFF0C\u6C89\u9189\u5728\u7231\u4E2D\u5427\uFF01",
    "style": "sp",
    "text": "\u5973\u5B50\uFF1A",
    "style": "q1",
    "text": "\n    ",
    "v": "2",
    "text": "\u6211\u867D\u7136\u8EBA\u5728\u5E8A\u4E0A\uFF0C\u5FC3\u5374\u6E05\u9192\u3002",
    "style": "q1",
    "text": "\u6211\u542C\u89C1\u6211\u7684\u826F\u4EBA\u53E9\u95E8\uFF0C\u8BF4\uFF1A",
    "style": "q1",
    "text": "\u201C\u5F00\u95E8\u5427\uFF01\u6211\u7684\u59B9\u59B9\uFF0C\u6211\u7684\u7231\u4EBA\uFF0C",
    "style": "q1",
    "text": "\u6211\u5B8C\u7F8E\u65E0\u7455\u7684\u5C0F\u9E3D\u5B50\u554A\uFF0C",
    "style": "q1",
    "text": "\u6211\u7684\u5934\u4E0A\u6CBE\u6EE1\u4E86\u9732\u6C34\uFF0C",
    "style": "q1",
    "text": "\u5934\u53D1\u88AB\u591C\u95F4\u7684\u9732\u73E0\u5F04\u6E7F\u4E86\u3002\u201D",
    "style": "q1",
    "text": "\n    ",
    "v": "3",
    "text": "\u53EF\u662F\u6211\u5DF2\u7ECF\u8131\u4E86\u8863\u88F3\uFF0C\u600E\u80FD\u518D\u7A7F\u4E0A\u5462\uFF1F",
    "style": "q1",
    "text": "\u6211\u5DF2\u7ECF\u6D17\u4E86\u811A\uFF0C\u600E\u80FD\u518D\u6CBE\u571F\u5462\uFF1F",
    "style": "q1",
    "text": "\n    ",
    "v": "4",
    "text": "\u6211\u7684\u826F\u4EBA\u4ECE\u95E8\u5B54\u4F38\u8FDB\u624B\u6765\uFF0C",
    "style": "q1",
    "text": "\u6211\u7684\u5FC3\u795E\u968F\u4E4B\u8361\u6F3E\uFF0C",
    "style": "q1",
