    "style": "q1",
    "text": "\n    ",
    "v": "2",
    "text": "\u8036\u548C\u534E\u8BF4\uFF1A",
    "style": "q1",
    "text": "\u201C\u6211\u5FC5\u6BC1\u706D\u5730\u4E0A\u7684\u4E00\u5207\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "3",
    "text": "\u6211\u5FC5\u6BC1\u706D\u4EBA\u7C7B\u3001\u517D\u7C7B\u3001",
    "style": "q1",
    "text": "\u5929\u4E0A\u7684\u9E1F\u548C\u6D77\u91CC\u7684\u9C7C\u3002",
    "style": "q1",
    "text": "\u6211\u5FC5\u4F7F\u6076\u4EBA\u5012\u6BD9\uFF0C",
    "style": "q1",
    "text": "\u6211\u5FC5\u94F2\u9664\u5730\u4E0A\u7684\u4EBA\u7C7B\u3002",
    "style": "q1",
    "text": "\u8FD9\u662F\u8036\u548C\u534E\u8BF4\u7684\u3002",
    "style": "b",
    "style": "q1",
    "text": "\n    ",
    "v": "4",
    "text": "\u201C\u6211\u5FC5\u4F38\u624B\u653B\u51FB",
    "pn": "\u72B9\u5927",
    "text": "\u72B9\u5927",
    "text": "\uFF0C",
    "style": "q1",
    "text": "\u653B\u51FB",
    "pn": "\u8036\u8DEF\u6492\u51B7",
    "text": "\u8036\u8DEF\u6492\u51B7",
    "text": "\u7684\u6240\u6709\u5C45\u6C11\uFF0C",
    "style": "q1",
    "text": "\u6E05\u9664",
    "pn": "\u5DF4\u529B",
    "text": "\u5DF4\u529B",
    "text": "\u7684\u6B8B\u8FF9\u53CA\u62DC\u5076\u50CF\u4E4B\u796D\u53F8\u7684\u540D\u53F7\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "5",
    "text": "\u6211\u5FC5\u94F2\u9664\u90A3\u4E9B\u5728\u5C4B\u9876\u796D\u62DC\u5929\u4E0A\u4E07\u8C61\u7684\u4EBA\uFF0C",
    "style": "q1",
    "text": "\u94F2\u9664\u656C\u62DC\u8036\u548C\u534E\u5E76\u51ED\u7942\u8D77\u8A93\u53C8\u51ED",
    "pn": "\u7C73\u52D2\u516C",
    "text": "\u7C73\u52D2\u516C",
    "text": "\u8D77\u8A93\u7684\u4EBA\uFF0C",
    "style": "q1",
    "text": "\n    ",
    "v": "6",
    "text": "\u94F2\u9664\u79BB\u5F03\u8036\u548C\u534E\u3001\u4E0D\u5BFB\u6C42\u7942\u3001\u4E0D\u6C42\u95EE\u7942\u7684\u4EBA\u3002\u201D",
    "style": "b",
    "style": "q1",
    "text": "\n    ",
    "v": "7",
    "text": "\u8981\u5728\u4E3B\u8036\u548C\u534E\u9762\u524D\u8083\u9759\uFF0C",
    "style": "q1",
    "text": "\u56E0\u4E3A\u8036\u548C\u534E\u7684\u65E5\u5B50\u8FD1\u4E86\u3002",
    "style": "q1",
    "text": "\u8036\u548C\u534E\u5DF2\u51C6\u5907\u597D\u796D\u7269\uFF0C",
    "style": "q1",
    "text": "\u6D01\u51C0\u4E86\u7942\u9080\u8BF7\u7684\u4EBA\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "8",
    "text": "\u8036\u548C\u534E\u8BF4\uFF1A\u201C\u5728\u7ED9\u8036\u548C\u534E\u732E\u796D\u4E4B\u65E5\uFF0C",
    "style": "q1",
    "text": "\u6211\u5FC5\u60E9\u7F5A\u9996\u9886\u548C\u738B\u5B50\uFF0C",
    "style": "q1",
    "text": "\u4EE5\u53CA\u6240\u6709\u7A7F\u5916\u65CF\u670D\u88C5\u7684\u4EBA\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "9",
    "text": "\u5230\u90A3\u65E5\uFF0C\u6211\u5FC5\u60E9\u7F5A\u6240\u6709\u8DF3\u8FC7\u95E8\u69DB",
    "text": "\u3001",
    "style": "q1",
    "text": "\u4F7F\u4E3B\u4EBA\u7684\u5BB6\u5145\u6EE1\u66B4\u529B\u548C\u6B3A\u8BC8\u7684\u4EBA\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "10",
    "text": "\u5230\u90A3\u65E5\uFF0C\u9C7C\u95E8\u5FC5\u4F20\u51FA\u54ED\u558A\u58F0\uFF0C",
    "style": "q1",
    "text": "\u65B0\u533A\u5FC5\u54CD\u8D77\u54C0\u53F7\u58F0\uFF0C",
    "style": "q1",
    "text": "\u5C71\u9675\u5FC5\u53D1\u51FA\u5D29\u88C2\u7684\u5DE8\u54CD\u3002",
    "style": "q1",
    "text": "\u8FD9\u662F\u8036\u548C\u534E\u8BF4\u7684\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "11",
    "text": "\u5E02\u573A\u533A\u7684\u5C45\u6C11\u554A\uFF0C\u54C0\u54ED\u5427\uFF01",
    "style": "q1",
    "text": "\u56E0\u4E3A\u6240\u6709\u7684\u5546\u4EBA\u5FC5\u706D\u4EA1\uFF0C",
    "style": "q1",
    "text": "\u6240\u6709\u505A\u4E70\u5356\u7684\u5FC5\u88AB\u94F2\u9664\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "12",
    "text": "\u90A3\u65F6\uFF0C\u6211\u5FC5\u63D0\u7740\u706F\u5DE1\u5BDF",
    "pn": "\u8036\u8DEF\u6492\u51B7",
    "text": "\u8036\u8DEF\u6492\u51B7",
    "text": "\uFF0C",
    "style": "q1",
    "text": "\u60E9\u7F5A\u90A3\u4E9B\u5B89\u4E8E\u7F6A\u4E2D\u7684\u4EBA\u3002",
    "style": "q1",
    "text": "\u4ED6\u4EEC\u5FC3\u60F3\uFF0C\u2018\u8036\u548C\u534E\u4E0D\u8D50\u798F\u4E5F\u4E0D\u964D\u7978\u3002\u2019",
    "style": "q1",
    "text": "\n    ",
    "v": "13",
    "text": "\u4ED6\u4EEC\u7684\u8D22\u7269\u5FC5\u906D\u62A2\u63A0\uFF0C",
    "style": "q1",
    "text": "\u5BB6\u56ED\u5FC5\u6CA6\u4E3A\u5E9F\u589F\u3002",
    "style": "q1",
    "text": "\u4ED6\u4EEC\u5EFA\u9020\u623F\u5C4B\uFF0C\u5374\u4E0D\u80FD\u4F4F\u5728\u91CC\u9762\uFF1B",
    "style": "q1",
    "text": "\u683D\u79CD\u8461\u8404\u56ED\uFF0C\u5374\u559D\u4E0D\u5230\u8461\u8404\u9152\u3002",
    "style": "b",
    "style": "q1",
    "text": "\n    ",
    "v": "14",
    "text": "\u201C\u8036\u548C\u534E\u7684\u5927\u65E5\u5B50\u8FD1\u4E86\uFF0C",
    "style": "q1",
    "text": "\u8FD1\u4E86\uFF0C\u5F88\u5FEB\u5C31\u5230\u4E86\u3002",
    "style": "q1",
    "text": "\u90A3\u5C06\u662F\u75DB\u82E6\u7684\u65E5\u5B50\uFF0C",
    "style": "q1",
    "text": "\u52C7\u58EB\u4E5F\u5FC5\u51C4\u58F0\u54C0\u53F7\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "15",
    "text": "\u90A3\u662F\u964D\u70C8\u6012\u7684\u65E5\u5B50\uFF0C",
    "style": "q1",
    "text": "\u662F\u56F0\u82E6\u8270\u96BE\u7684\u65E5\u5B50\uFF0C",
    "style": "q1",
    "text": "\u662F\u6467\u6B8B\u6BC1\u574F\u7684\u65E5\u5B50\uFF0C",
    "style": "q1",
    "text": "\u662F\u9ED1\u6697\u5E7D\u51A5\u7684\u65E5\u5B50\uFF0C",
    "style": "q1",
    "text": "\u662F\u9634\u973E\u5BC6\u5E03\u7684\u65E5\u5B50\uFF0C",
    "style": "q1",
    "text": "\n    ",
    "v": "16",
    "text": "\u662F\u5439\u53F7\u5450\u558A\u3001",
    "style": "q1",
    "text": "\u653B\u6253\u575A\u57CE\u9AD8\u5792\u7684\u65E5\u5B50\u3002",
    "style": "b",
    "style": "q1",
    "text": "\n    ",
    "v": "17",
    "text": "\u201C\u6211\u8981\u4F7F\u707E\u96BE\u4E34\u5230\u4EBA\u7C7B\uFF0C",
    "style": "q1",
    "text": "\u4EE5\u81F4\u4ED6\u4EEC\u884C\u8DEF\u50CF\u778E\u773C\u7684\uFF0C",
    "style": "q1",
    "text": "\u56E0\u4E3A\u4ED6\u4EEC\u5F97\u7F6A\u4E86\u8036\u548C\u534E\u3002",
    "style": "q1",
    "text": "\u4ED6\u4EEC\u7684\u8840\u5FC5\u5982\u7070\u5C18\u822C\u88AB\u5012\u51FA\uFF0C",
    "style": "q1",
    "text": "\u4ED6\u4EEC\u7684\u5C38\u4F53\u5FC5\u5982\u7CAA\u4FBF\u822C\u88AB\u4E22\u5F03\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "18",
    "text": "\u5728\u8036\u548C\u534E\u53D1\u6012\u7684\u65E5\u5B50\uFF0C",
    "style": "q1",
    "text": "\u4ED6\u4EEC\u7684\u91D1\u94F6\u6551\u4E0D\u4E86\u4ED6\u4EEC\u3002",
    "style": "q1",
    "text": "\u7942\u7684\u6012\u706B\u5FC5\u541E\u566C\u5927\u5730\uFF0C",
    "style": "q1",
    "text": "\u7942\u5FC5\u9AA4\u7136\u6BC1\u706D\u4E00\u5207\u4E16\u4EBA\u3002\u201D",
    "c": "2",
    "style": "s1",
    "text": "\u547C\u5401\u767E\u59D3\u6094\u6539",
    "style": "p",
    "text": "\n    ",
    "v": "1-2",
    "text": "\u4E0D\u77E5\u7F9E\u803B\u7684\u56FD\u6C11\u554A\uFF0C",
    "style": "q1",
    "text": "\u8D81\u547D\u4EE4\u5C1A\u672A\u53D1\u51FA\u3001",
    "style": "q1",
    "text": "\u65E5\u5B50\u5C1A\u672A\u6D88\u901D\u5982\u98CE\u4E2D\u7684\u7CE0\u79D5\u3001",
    "style": "q1",
    "text": "\u8036\u548C\u534E\u7684\u70C8\u6012\u5C1A\u672A\u4E34\u5230\u4F60\u4EEC\u3001",
    "style": "q1",
    "text": "\u7942\u5411\u4F60\u4EEC\u53D1\u6012\u7684\u65E5\u5B50\u5C1A\u672A\u6765\u4E34\uFF0C",
    "style": "q1",
    "text": "\u8981\u805A\u96C6\uFF0C\u805A\u96C6\u8D77\u6765\uFF01",
    "style": "q1",
    "text": "\n    ",
    "v": "3",
    "text": "\u4E16\u4E0A\u6240\u6709\u9075\u5B88\u8036\u548C\u534E\u5178\u7AE0\u7684\u8C26\u5351\u4EBA\u554A\uFF0C",
    "style": "q1",
    "text": "\u8981\u5BFB\u6C42\u8036\u548C\u534E\uFF0C\u8FFD\u6C42\u516C\u4E49\uFF0C\u8FFD\u6C42\u8C26\u5351\u3002",
    "style": "q1",
    "text": "\u4E5F\u8BB8\u5728\u8036\u548C\u534E\u53D1\u6012\u7684\u65E5\u5B50\uFF0C",
    "style": "q1",
    "text": "\u4F60\u4EEC\u53EF\u4EE5\u5F97\u5230\u5E87\u62A4\u3002",
    "style": "s1",
    "text": "\u5BA1\u5224\u4EE5\u8272\u5217\u7684\u90BB\u56FD",
    "style": "q1",
    "text": "\n    ",
    "v": "4",
    "text": "\n    ",
    "pn": "\u8FE6\u8428",
    "text": "\u8FE6\u8428",
