    "style": "q1",
    "text": "\n    ",
    "v": "8-9",
    "text": "\n    ",
    "pn": "\u4EE5\u8272\u5217",
    "text": "\u4EE5\u8272\u5217",
    "text": "\u7684\u4E0A\u5E1D\u2014\u2014\u4E07\u519B\u4E4B\u8036\u548C\u534E\u8BF4\uFF1A",
    "style": "q1",
    "text": "\u201C\u6211\u542C\u89C1\u4E86",
    "pn": "\u6469\u62BC",
    "text": "\u6469\u62BC",
    "text": "\u4EBA\u7684\u6BC1\u8C24\u548C",
    "pn": "\u4E9A\u626A",
    "text": "\u4E9A\u626A",
    "text": "\u4EBA\u7684\u8FB1\u9A82\u3002",
    "style": "q1",
    "text": "\u4ED6\u4EEC\u6BC1\u8C24\u6211\u7684\u5B50\u6C11\uFF0C\u626C\u8A00\u4FB5\u5360\u4ED6\u4EEC\u7684\u571F\u5730\u3002",
    "style": "q1",
    "text": "\u6211\u51ED\u81EA\u5DF1\u7684\u6C38\u6052\u8D77\u8A93\uFF0C",
    "style": "q1",
    "text": "\n    ",
    "pn": "\u6469\u62BC",
    "text": "\u6469\u62BC",
    "text": "\u548C",
    "pn": "\u4E9A\u626A",
    "text": "\u4E9A\u626A",
    "text": "\u5FC5\u523A\u8349\u4E1B\u751F\u3001\u76D0\u5751\u904D\u5730\u3001\u6C38\u8FDC\u8352\u5E9F\uFF0C",
    "style": "q1",
    "text": "\u5982",
    "pn": "\u6240\u591A\u739B",
    "text": "\u6240\u591A\u739B",
    "text": "\u548C",
    "pn": "\u86FE\u6469\u62C9",
    "text": "\u86FE\u6469\u62C9",
    "text": "\u4E00\u6837\u3002",
    "style": "q1",
    "text": "\u6211\u4F59\u4E0B\u7684\u5B50\u6C11\u5FC5\u63B3\u63A0\u4ED6\u4EEC\uFF0C",
    "style": "q1",
    "text": "\u6211\u5E78\u5B58\u7684\u767E\u59D3\u5FC5\u627F\u53D7\u4ED6\u4EEC\u7684\u571F\u5730\u3002\u201D",
    "style": "q1",
    "text": "\n    ",
    "v": "10",
    "text": "\u8FD9\u662F\u4ED6\u4EEC\u7684\u62A5\u5E94\uFF0C",
    "style": "q1",
    "text": "\u56E0\u4E3A\u4ED6\u4EEC\u72C2\u5984\u81EA\u5927\uFF0C",
    "style": "q1",
    "text": "\u7F9E\u8FB1\u4E07\u519B\u4E4B\u8036\u548C\u534E\u7684\u5B50\u6C11\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "11",
    "text": "\u8036\u548C\u534E\u5FC5\u4EE4\u4ED6\u4EEC\u6050\u60E7\uFF0C",
    "style": "q1",
    "text": "\u56E0\u4E3A\u7942\u8981\u524A\u5F31\u4E16\u4E0A\u7684\u795E\u660E\u3002",
    "style": "q1",
    "text": "\u5929\u4E0B\u4E07\u6C11\u5FC5\u5728\u672C\u5730\u656C\u62DC\u7942\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "12",
    "text": "\n    ",
    "pn": "\u53E4\u5B9E",
    "text": "\u53E4\u5B9E",
    "text": "\u4EBA\u554A\uFF0C",
    "style": "q1",
    "text": "\u4F60\u4EEC\u4E5F\u5FC5\u6B7B\u5728\u8036\u548C\u534E\u7684\u5200\u4E0B\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "13",
    "text": "\u7942\u5FC5\u4F38\u624B\u653B\u51FB\u5317\u65B9\uFF0C\u6BC1\u706D",
    "pn": "\u4E9A\u8FF0",
    "text": "\u4E9A\u8FF0",
    "text": "\uFF0C",
    "style": "q1",
    "text": "\u4F7F",
    "pn": "\u5C3C\u5C3C\u5FAE",
    "text": "\u5C3C\u5C3C\u5FAE",
    "text": "\u4E00\u7247\u8352\u51C9\uFF0C",
    "style": "q1",
    "text": "\u5982\u5E72\u65F1\u7684\u65F7\u91CE\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "14",
    "text": "\u7272\u755C\u548C\u91CE\u517D\u5FC5\u8EBA\u5367\u5728\u90A3\u91CC\uFF0C",
    "style": "q1",
    "text": "\u732B\u5934\u9E70\u548C\u523A\u732C\u6816\u8EAB\u5728\u623F\u67F1\u4E0A\uFF0C",
    "style": "q1",
    "text": "\u7A97\u53E3\u4F20\u51FA\u9E1F\u9E23\uFF0C",
    "style": "q1",
    "text": "\u95E8\u5EAD\u90FD\u5FC5\u7834\u8D25\uFF0C",
    "style": "q1",
    "text": "\u9732\u51FA\u9999\u67CF\u6728\u6881\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "15",
    "text": "\u8FD9\u5C31\u662F\u90A3\u6B22\u4E50\u5B89\u9038\u3001",
    "style": "q1",
    "text": "\u81EA\u4EE5\u4E3A\u5929\u4E0B\u65E0\u53CC\u7684\u57CE\uFF0C",
    "style": "q1",
    "text": "\u73B0\u5728\u7ADF\u7136\u5982\u6B64\u8352\u51C9\uFF0C",
    "style": "q1",
    "text": "\u6210\u4E3A\u91CE\u517D\u6816\u606F\u4E4B\u5730\uFF01",
    "style": "q1",
    "text": "\u8DEF\u8FC7\u7684\u4EBA\u90FD\u6447\u624B\u5632\u7B11\u5B83\u3002",
    "c": "3",
    "style": "s1",
    "text": "\u8036\u8DEF\u6492\u51B7\u7684\u8D25\u574F",
    "style": "q1",
    "text": "\n    ",
    "v": "1",
    "text": "\u8FD9\u53DB\u9006\u3001\u6C61\u79FD\u3001\u5145\u6EE1\u538B\u8FEB\u7684\u57CE\u6709\u7978\u4E86\uFF01",
    "style": "q1",
    "text": "\n    ",
    "v": "2",
    "text": "\u5979\u4E0D\u542C\u547D\u4EE4\uFF0C\u4E0D\u53D7\u7BA1\u6559\uFF1B",
    "style": "q1",
    "text": "\u4E0D\u501A\u9760\u8036\u548C\u534E\uFF0C",
    "style": "q1",
    "text": "\u4E0D\u4EB2\u8FD1\u5979\u7684\u4E0A\u5E1D\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "3",
    "text": "\u5979\u7684\u9996\u9886\u662F\u5486\u54EE\u7684\u72EE\u5B50\uFF0C",
    "style": "q1",
    "text": "\u5979\u7684\u5BA1\u5224\u5B98\u662F\u591C\u95F4\u7684\u8C7A\u72FC\uFF0C",
    "style": "q1",
    "text": "\u4E0D\u7559\u4E00\u70B9\u730E\u7269\u5230\u5929\u660E\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "4",
    "text": "\u5979\u7684\u5148\u77E5\u72C2\u5984\u8BE1\u8BC8\uFF0C",
    "style": "q1",
    "text": "\u5979\u7684\u796D\u53F8\u4EB5\u6E0E\u5723\u6BBF\uFF0C\u8FDD\u80CC\u5F8B\u6CD5\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "5",
    "text": "\u4F46\u57CE\u4E2D\u7684\u8036\u548C\u534E\u662F\u516C\u4E49\u7684\uFF0C",
    "style": "q1",
    "text": "\u7942\u7EDD\u4E0D\u505A\u4E0D\u4E49\u4E4B\u4E8B\u3002",
    "style": "q1",
    "text": "\u7942\u6BCF\u5929\u65E9\u6668\u4F38\u5F20\u6B63\u4E49\uFF0C\u4ECE\u4E0D\u95F4\u65AD\u3002",
    "style": "q1",
    "text": "\u7136\u800C\uFF0C\u4E0D\u4E49\u4E4B\u4EBA\u5374\u4E0D\u77E5\u7F9E\u803B\u3002",
    "style": "b",
    "style": "q1",
    "text": "\n    ",
    "v": "6",
    "text": "\u201C\u6211\u5DF2\u6D88\u706D\u5217\u56FD\uFF0C",
    "style": "q1",
    "text": "\u6467\u6BC1\u4ED6\u4EEC\u7684\u57CE\u697C\uFF0C",
    "style": "q1",
    "text": "\u4F7F\u4ED6\u4EEC\u7684\u8857\u9053\u8352\u5E9F\uFF0C\u6CA1\u6709\u8DEF\u4EBA\uFF1B",
    "style": "q1",
    "text": "\u4ED6\u4EEC\u7684\u57CE\u9091\u6CA6\u4E3A\u5E9F\u589F\uFF0C\u6773\u65E0\u4EBA\u8FF9\uFF0C\u6CA1\u6709\u5C45\u6C11\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "7",
    "text": "\u6211\u4EE5\u4E3A\u8FD9\u57CE\u4E00\u5B9A\u4F1A\u656C\u754F\u6211\uFF0C\u63A5\u53D7\u7BA1\u6559\uFF0C",
    "style": "q1",
    "text": "\u8FD9\u6837\u6211\u5C31\u4E0D\u5FC5\u7167\u6240\u5B9A\u7684\u6BC1\u706D\u5979\u7684\u5C45\u6240\u3002",
    "style": "q1",
    "text": "\u7136\u800C\uFF0C\u4ED6\u4EEC\u5374\u66F4\u52A0\u70ED\u8877\u4E8E\u72AF\u7F6A\u4F5C\u6076\u3002",
    "style": "b",
    "style": "q1",
    "text": "\n    ",
    "v": "8",
    "text": "\u201C\u56E0\u6B64\uFF0C\u8981\u7B49\u5019\u6211\uFF0C",
    "style": "q1",
    "text": "\u7B49\u5019\u6211\u8D77\u6765\u6307\u63A7\u7684\u65E5\u5B50\u3002",
    "style": "q1",
    "text": "\u6211\u5DF2\u51B3\u5B9A\u53EC\u96C6\u5217\u56FD\uFF0C\u6C47\u805A\u5217\u90A6\uFF0C",
    "style": "q1",
    "text": "\u5411\u4ED6\u4EEC\u503E\u5012\u6211\u7684\u70C8\u6012\uFF0C",
    "style": "q1",
    "text": "\u6211\u7684\u6012\u706B\u5FC5\u541E\u566C\u5929\u4E0B\u3002",
    "style": "q1",
    "text": "\u8FD9\u662F\u8036\u548C\u534E\u8BF4\u7684\u3002",
    "style": "s1",
    "text": "\u8036\u548C\u534E\u7684\u62EF\u6551",
    "style": "q1",
    "text": "\n    ",
    "v": "9",
    "text": "\u201C\u90A3\u65F6\uFF0C\u6211\u5FC5\u6D01\u51C0\u4E07\u6C11\u7684\u53E3\uFF0C",
    "style": "q1",
    "text": "\u4ED6\u4EEC\u597D\u6C42\u544A\u6211\u7684\u540D\uFF0C",
    "style": "q1",
    "text": "\u540C\u5FC3\u5408\u610F\u5730\u4E8B\u5949\u6211\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "10",
    "text": "\u90A3\u4E9B\u656C\u62DC\u6211\u7684\uFF0C",
    "style": "q1",
    "text": "\u5C31\u662F\u6211\u90A3\u4E9B\u5206\u6563\u7684\u5B50\u6C11\u5FC5\u4ECE",
    "pn": "\u53E4\u5B9E",
    "text": "\u53E4\u5B9E",
    "text": "\u6CB3\u5BF9\u5CB8\u5E26\u7740\u4F9B\u7269\u6765\u732E\u7ED9\u6211\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "11",
    "text": "\u5230\u90A3\u5929\uFF0C\u4F60\u5FC5\u4E0D\u518D\u56E0\u80CC\u53DB\u6211\u800C\u7F9E\u6127\uFF0C",
    "style": "q1",
    "text": "\u56E0\u4E3A\u90A3\u65F6\u6211\u5FC5\u9664\u6389\u4F60\u90A3\u91CC\u7684\u72C2\u50B2\u4E4B\u5F92\uFF0C",
    "style": "q1",
    "text": "\u4F60\u5728\u6211\u7684\u5723\u5C71\u4E0A\u5FC5\u4E0D\u518D\u9AD8\u50B2\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "12",
    "text": "\u6211\u5FC5\u628A\u8C26\u5351\u8D2B\u82E6\u7684\u4EBA\u7559\u5728\u4F60\u90A3\u91CC\uFF0C",
    "style": "q1",
    "text": "\u4ED6\u4EEC\u5FC5\u6295\u9760\u5728\u8036\u548C\u534E\u7684\u540D\u4E0B\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "13",
    "text": "\n    ",
    "pn": "\u4EE5\u8272\u5217",
    "text": "\u4EE5\u8272\u5217",
    "text": "\u7684\u4F59\u6C11\u5FC5\u4E0D\u518D\u4F5C\u6076\uFF0C",
    "style": "q1",
    "text": "\u4E0D\u518D\u6492\u8C0E\uFF0C\u53E3\u4E2D\u4E0D\u518D\u51FA\u8BE1\u8BC8\u4E4B\u8A00\u3002",
    "style": "q1",
    "text": "\u4ED6\u4EEC\u5403\u559D\u8EBA\u5367\uFF0C\u65E0\u4EBA\u60CA\u6270\u3002\u201D",
    "style": "s1",
    "text": "\u6B22\u4E50\u4E4B\u6B4C",
    "style": "q1",
    "text": "\n    ",
    "v": "14",
    "text": "\u5C11\u5973",
    "pn": "\u9521\u5B89",
    "text": "\u9521\u5B89",
    "text": "\u554A\uFF0C\u6B4C\u5531\u5427\uFF01",
    "style": "q1",
    "text": "\n    ",
    "pn": "\u4EE5\u8272\u5217",
    "text": "\u4EE5\u8272\u5217",
    "text": "\u554A\uFF0C\u6B22\u547C\u5427\uFF01",
    "style": "q1",
    "text": "\u5C11\u5973",
    "pn": "\u8036\u8DEF\u6492\u51B7",
    "text": "\u8036\u8DEF\u6492\u51B7",
    "text": "\u554A\uFF0C\u5C3D\u60C5\u5730\u6B22\u559C\u5FEB\u4E50\u5427\uFF01",
    "style": "q1",
    "text": "\n    ",
    "v": "15",
    "text": "\u8036\u548C\u534E\u5DF2\u7ECF\u64A4\u9500\u5BF9\u4F60\u7684\u5211\u7F5A\uFF0C",
    "style": "q1",
    "text": "\u8D76\u8D70\u4F60\u7684\u4EC7\u654C\uFF0C",
    "style": "q1",
    "text": "\n    ",
    "pn": "\u4EE5\u8272\u5217",
    "text": "\u4EE5\u8272\u5217",
    "text": "\u7684\u738B\u8036\u548C\u534E\u4E0E\u4F60\u540C\u5728\uFF0C",
    "style": "q1",
    "text": "\u4F60\u5FC5\u4E0D\u518D\u60E7\u6015\u707E\u7978\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "16",
    "text": "\u5230\u90A3\u5929\uFF0C",
    "pn": "\u8036\u8DEF\u6492\u51B7",
    "text": "\u8036\u8DEF\u6492\u51B7",
    "text": "\u5FC5\u542C\u5230\u8FD9\u6837\u7684\u8BDD\uFF1A",
    "style": "q1",
    "text": "\u201C",
    "pn": "\u9521\u5B89",
    "text": "\u9521\u5B89",
    "text": "\u554A\uFF0C\u4E0D\u8981\u5BB3\u6015\uFF01\u4E0D\u8981\u53CC\u624B\u53D1\u8F6F\uFF01",
    "style": "q1",
    "text": "\n    ",
    "v": "17",
    "text": "\u4F60\u7684\u4E0A\u5E1D\u8036\u548C\u534E\u4E0E\u4F60\u540C\u5728\uFF0C",
    "style": "q1",
    "text": "\u7942\u662F\u5927\u80FD\u7684\u62EF\u6551\u8005\uFF0C",
    "style": "q1",
    "text": "\u7942\u5FC5\u56E0\u4F60\u800C\u6B22\u559C\u5FEB\u4E50\uFF0C\u6B22\u7136\u6B4C\u5531\uFF0C",
    "style": "q1",
    "text": "\u7528\u7942\u7684\u7231\u5B89\u6170\u4F60\u3002\u201D",
    "style": "b",
    "style": "q1",
    "text": "\n    ",
    "v": "18",
    "text": "\u201C\u6211\u5FC5\u62DB\u805A\u4F60\u90A3\u91CC\u56E0\u4E0D\u80FD\u5B88\u8282\u671F\u800C\u5FE7\u6101\u7684\u4EBA\uFF0C",
    "style": "q1",
    "text": "\u4F7F\u4F60\u4E0D\u518D\u8499\u53D7\u7F9E\u8FB1\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "19",
    "text": "\u770B\u554A\uFF0C\u90A3\u65F6\u6211\u5FC5\u60E9\u7F5A\u6240\u6709\u6B3A\u538B\u4F60\u7684\u4EBA\uFF0C",
    "style": "q1",
    "text": "\u62EF\u6551\u7638\u817F\u7684\uFF0C\u53EC\u56DE\u5206\u6563\u7684\uFF0C",
    "style": "q1",
    "text": "\u4F7F\u4ED6\u4EEC\u5728\u666E\u5929\u4E4B\u4E0B\u7684\u7F9E\u8FB1\u5316\u4E3A\u8D5E\u7F8E\u548C\u5C0A\u8363\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "20",
    "text": "\u90A3\u65F6\uFF0C\u6211\u5FC5\u62DB\u805A\u4F60\u4EEC\uFF0C\u5E26\u4F60\u4EEC\u56DE\u5230\u6545\u571F\u3002",
    "style": "q1",
    "text": "\u4F60\u4EEC\u4EB2\u773C\u76EE\u7779\u6211\u5E26\u4F60\u4EEC\u56DE\u5230\u6545\u571F\u540E\uFF0C",
    "style": "q1",
    "text": "\u6211\u5FC5\u4F7F\u4F60\u4EEC\u5728\u5929\u4E0B\u4E07\u6C11\u4E2D\u58F0\u540D\u8FDC\u626C\uFF0C\u5F97\u4EAB\u5C0A\u8363\u3002\u201D",
    "style": "q1",
    "text": "\u8FD9\u662F\u8036\u548C\u534E\u8BF4\u7684\u3002"
  ]
}