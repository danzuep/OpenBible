    "style": "q1",
    "text": "\n    ",
    "v": "5",
    "text": "\u4F4F\u5728\u6D77\u8FB9\u7684",
    "pn": "\u57FA\u5229\u63D0",
    "text": "\u57FA\u5229\u63D0",
    "text": "\u4EBA\u554A\uFF0C",
    "style": "q1",
    "text": "\u4F60\u4EEC\u6709\u7978\u4E86\uFF01",
    "style": "q1",
    "text": "\n    ",
    "pn": "\u975E\u5229\u58EB",
    "text": "\u975E\u5229\u58EB",
    "text": "\u4EBA\u7684",
    "pn": "\u8FE6\u5357",
    "text": "\u8FE6\u5357",
    "text": "\u554A\uFF0C",
    "style": "q1",
    "text": "\u8036\u548C\u534E\u5BA3\u544A\u8981\u6BC1\u706D\u4F60\uFF0C",
    "style": "q1",
    "text": "\u4F7F\u4F60\u6773\u65E0\u4EBA\u70DF\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "6",
    "text": "\u6CBF\u6D77\u5730\u533A\u5FC5\u6210\u4E3A\u8349\u573A\uFF0C",
    "style": "q1",
    "text": "\u6709\u7267\u4EBA\u7684\u5C45\u6240\u548C\u7F8A\u5708\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "7",
    "text": "\u90A3\u91CC\u5FC5\u5F52\u7ED9",
    "pn": "\u72B9\u5927",
    "text": "\u72B9\u5927",
    "text": "\u5BB6\u7684\u4F59\u6C11\uFF0C",
    "style": "q1",
    "text": "\u4ED6\u4EEC\u5FC5\u5728\u90A3\u91CC\u653E\u7267\uFF0C",
    "style": "q1",
    "text": "\u665A\u4E0A\u7761\u5728",
