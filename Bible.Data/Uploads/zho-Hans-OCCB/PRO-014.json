[["c","14"],["v","1"],["","智慧的女子建立家室，"],["q2","愚昧的女子亲手拆毁。"],["v","2"],["","行为正直的人敬畏耶和华，"],["q2","行事邪僻的人轻视耶和华。"],["v","3"],["","愚人的狂言招来鞭打，"],["q2","智者的唇舌保护自己。"],["v","4"],["","没有耕牛槽头净，"],["q2","五谷丰登需壮牛。"],["v","5"],["","忠实的证人不会撒谎，"],["q2","虚假的证人谎话连篇。"],["v","6"],["","嘲讽者徒然寻智慧，"],["q2","明哲轻易得知识。"],["v","7"],["","你要远离愚昧人，"],["q2","他口中毫无知识。"],["v","8"],["","明哲凭智慧辨道，"],["q2","愚人被愚昧欺骗。"],["v","9"],["","愚妄人戏看罪恶，"],["q2","正直人彼此恩待。"],["v","10"],["","心头的愁苦，唯有自己明白；"],["q2","心中的喜乐，外人无法分享。"],["v","11"],["","恶人的房屋必遭毁灭，"],["q2","正直人的帐篷必兴盛。"],["v","12"],["","有一条路看似正确，"],["q2","最终却通向死亡。"],["v","13"],["","欢笑难消内心的痛苦，"],["q2","欢乐过后，悲伤犹在。"],["v","14"],["","背弃正道，自食恶果；"],["q2","善人行善，必得善报。"],["v","15"],["","愚人什么都信，"],["q2","明哲步步谨慎。"],["v","16"],["","智者小心谨慎，远离恶事；"],["q2","愚人骄傲自负，行事鲁莽。"],["v","17"],["","急躁易怒的人做事愚昧，"],["q2","阴险奸诈之人遭人痛恨。"],["v","18"],["","愚人得愚昧作产业，"],["q2","明哲得知识为冠冕。"],["v","19"],["","坏人俯伏在善人面前，"],["q2","恶人俯伏在义人门口。"],["v","20"],["","穷人遭邻舍厌，"],["q2","富人朋友众多。"],["v","21"],["","藐视邻舍是罪过，"],["q2","怜悯穷人蒙福乐。"],["v","22"],["","图谋恶事的步入歧途，"],["q2","行善的得慈爱与忠诚。"],["v","23"],["","殷勤工作，带来益处；"],["q2","满嘴空谈，导致贫穷。"],["v","24"],["","智者以财富为冠冕，"],["q2","愚人以愚昧为装饰。"],["v","25"],["","诚实的证人挽救性命，"],["q2","口吐谎言者欺骗他人。"],["v","26"],["","敬畏耶和华的信心坚定，"],["q2","他的子孙也有庇护所。"],["v","27"],["","敬畏耶和华是生命之泉，"],["q2","可使人避开死亡的陷阱。"],["v","28"],["","人口众多，是君王的荣耀；"],["q2","没有臣民，君主必然败亡。"],["v","29"],["","不轻易发怒者深明事理，"],["q2","鲁莽急躁的人显出愚昧。"],["v","30"],["","心平气和，滋润生命；"],["q2","妒火中烧，啃蚀骨头。"],["v","31"],["","欺压穷人等于侮辱造物主，"],["q2","怜悯贫弱就是尊敬造物主。"],["v","32"],["","恶人因恶行而灭亡，"],["q2","义人到死仍有倚靠。"],["v","33"],["","智慧存在哲士心里，"],["q2","愚人心中充满无知。"],["v","34"],["","公义能叫邦国兴盛，"],["q2","罪恶是人民的耻辱。"],["v","35"],["","明智的臣子蒙王喜悦，"],["q2","可耻的仆人惹王发怒。"]]