    "pn": "\u6240\u591A\u746A",
    "text": "\u6240\u591A\u746A",
    "text": "\u548C",
    "pn": "\u86FE\u6469\u62C9",
    "text": "\u86FE\u6469\u62C9",
    "text": "\u4E00\u6A23\u3002",
    "style": "q1",
    "text": "\u6211\u9918\u4E0B\u7684\u5B50\u6C11\u5FC5\u64C4\u63A0\u4ED6\u5011\uFF0C",
    "style": "q1",
    "text": "\u6211\u5016\u5B58\u7684\u767E\u59D3\u5FC5\u627F\u53D7\u4ED6\u5011\u7684\u571F\u5730\u3002\u300D",
    "style": "q1",
    "text": "\n    ",
    "v": "10",
    "text": "\u9019\u662F\u4ED6\u5011\u7684\u5831\u61C9\uFF0C",
    "style": "q1",
    "text": "\u56E0\u70BA\u4ED6\u5011\u72C2\u5984\u81EA\u5927\uFF0C",
    "style": "q1",
    "text": "\u7F9E\u8FB1\u842C\u8ECD\u4E4B\u8036\u548C\u83EF\u7684\u5B50\u6C11\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "11",
    "text": "\u8036\u548C\u83EF\u5FC5\u4EE4\u4ED6\u5011\u6050\u61FC\uFF0C",
    "style": "q1",
    "text": "\u56E0\u70BA\u7942\u8981\u524A\u5F31\u4E16\u4E0A\u7684\u795E\u660E\u3002",
    "style": "q1",
    "text": "\u5929\u4E0B\u842C\u6C11\u5FC5\u5728\u672C\u5730\u656C\u62DC\u7942\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "12",
    "text": "\n    ",
    "pn": "\u53E4\u5BE6",
    "text": "\u53E4\u5BE6",
    "text": "\u4EBA\u554A\uFF0C",
    "style": "q1",
    "text": "\u4F60\u5011\u4E5F\u5FC5\u6B7B\u5728\u8036\u548C\u83EF\u7684\u5200\u4E0B\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "13",
    "text": "\u7942\u5FC5\u4F38\u624B\u653B\u64CA\u5317\u65B9\uFF0C\u6BC0\u6EC5",
    "pn": "\u4E9E\u8FF0",
    "text": "\u4E9E\u8FF0",
    "text": "\uFF0C",
    "style": "q1",
    "text": "\u4F7F",
    "pn": "\u5C3C\u5C3C\u5FAE",
    "text": "\u5C3C\u5C3C\u5FAE",
    "text": "\u4E00\u7247\u8352\u6DBC\uFF0C",
    "style": "q1",
    "text": "\u5982\u4E7E\u65F1\u7684\u66E0\u91CE\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "14",
    "text": "\u7272\u755C\u548C\u91CE\u7378\u5FC5\u8EBA\u81E5\u5728\u90A3\u88E1\uFF0C",
    "style": "q1",
    "text": "\u8C93\u982D\u9DF9\u548C\u523A\u875F\u68F2\u8EAB\u5728\u623F\u67F1\u4E0A\uFF0C",
    "style": "q1",
    "text": "\u7A97\u53E3\u50B3\u51FA\u9CE5\u9CF4\uFF0C",
    "style": "q1",
    "text": "\u9580\u5EAD\u90FD\u5FC5\u7834\u6557\uFF0C",
    "style": "q1",
    "text": "\u9732\u51FA\u9999\u67CF\u6728\u6881\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "15",
    "text": "\u9019\u5C31\u662F\u90A3\u6B61\u6A02\u5B89\u9038\u3001",
    "style": "q1",
    "text": "\u81EA\u4EE5\u70BA\u5929\u4E0B\u7121\u96D9\u7684\u57CE\uFF0C",
    "style": "q1",
    "text": "\u73FE\u5728\u7ADF\u7136\u5982\u6B64\u8352\u6DBC\uFF0C",
    "style": "q1",
    "text": "\u6210\u70BA\u91CE\u7378\u68F2\u606F\u4E4B\u5730\uFF01",
    "style": "q1",
    "text": "\u8DEF\u904E\u7684\u4EBA\u90FD\u6416\u624B\u5632\u7B11\u5B83\u3002",
    "c": "3",
    "style": "s1",
    "text": "\u8036\u8DEF\u6492\u51B7\u7684\u6557\u58DE",
    "style": "q1",
    "text": "\n    ",
    "v": "1",
    "text": "\u9019\u53DB\u9006\u3001\u6C61\u7A62\u3001\u5145\u6EFF\u58D3\u8FEB\u7684\u57CE\u6709\u798D\u4E86\uFF01",
    "style": "q1",
    "text": "\n    ",
    "v": "2",
    "text": "\u5979\u4E0D\u807D\u547D\u4EE4\uFF0C\u4E0D\u53D7\u7BA1\u6559\uFF1B",
    "style": "q1",
    "text": "\u4E0D\u501A\u9760\u8036\u548C\u83EF\uFF0C",
    "style": "q1",
    "text": "\u4E0D\u89AA\u8FD1\u5979\u7684\u4E0A\u5E1D\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "3",
    "text": "\u5979\u7684\u9996\u9818\u662F\u5486\u54EE\u7684\u7345\u5B50\uFF0C",
    "style": "q1",
    "text": "\u5979\u7684\u5BE9\u5224\u5B98\u662F\u591C\u9593\u7684\u8C7A\u72FC\uFF0C",
    "style": "q1",
    "text": "\u4E0D\u7559\u4E00\u9EDE\u7375\u7269\u5230\u5929\u660E\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "4",
    "text": "\u5979\u7684\u5148\u77E5\u72C2\u5984\u8A6D\u8A50\uFF0C",
    "style": "q1",
    "text": "\u5979\u7684\u796D\u53F8\u893B\u7006\u8056\u6BBF\uFF0C\u9055\u80CC\u5F8B\u6CD5\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "5",
    "text": "\u4F46\u57CE\u4E2D\u7684\u8036\u548C\u83EF\u662F\u516C\u7FA9\u7684\uFF0C",
    "style": "q1",
    "text": "\u7942\u7D55\u4E0D\u505A\u4E0D\u7FA9\u4E4B\u4E8B\u3002",
    "style": "q1",
    "text": "\u7942\u6BCF\u5929\u65E9\u6668\u4F38\u5F35\u6B63\u7FA9\uFF0C\u5F9E\u4E0D\u9593\u65B7\u3002",
    "style": "q1",
    "text": "\u7136\u800C\uFF0C\u4E0D\u7FA9\u4E4B\u4EBA\u537B\u4E0D\u77E5\u7F9E\u6065\u3002",
    "style": "b",
    "style": "q1",
    "text": "\n    ",
    "v": "6",
    "text": "\u300C\u6211\u5DF2\u6D88\u6EC5\u5217\u570B\uFF0C",
    "style": "q1",
    "text": "\u6467\u6BC0\u4ED6\u5011\u7684\u57CE\u6A13\uFF0C",
    "style": "q1",
    "text": "\u4F7F\u4ED6\u5011\u7684\u8857\u9053\u8352\u5EE2\uFF0C\u6C92\u6709\u8DEF\u4EBA\uFF1B",
    "style": "q1",
    "text": "\u4ED6\u5011\u7684\u57CE\u9091\u6DEA\u70BA\u5EE2\u589F\uFF0C\u6773\u7121\u4EBA\u8DE1\uFF0C\u6C92\u6709\u5C45\u6C11\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "7",
    "text": "\u6211\u4EE5\u70BA\u9019\u57CE\u4E00\u5B9A\u6703\u656C\u754F\u6211\uFF0C\u63A5\u53D7\u7BA1\u6559\uFF0C",
    "style": "q1",
    "text": "\u9019\u6A23\u6211\u5C31\u4E0D\u5FC5\u7167\u6240\u5B9A\u7684\u6BC0\u6EC5\u5979\u7684\u5C45\u6240\u3002",
    "style": "q1",
    "text": "\u7136\u800C\uFF0C\u4ED6\u5011\u537B\u66F4\u52A0\u71B1\u8877\u65BC\u72AF\u7F6A\u4F5C\u60E1\u3002",
    "style": "b",
    "style": "q1",
    "text": "\n    ",
    "v": "8",
    "text": "\u300C\u56E0\u6B64\uFF0C\u8981\u7B49\u5019\u6211\uFF0C",
    "style": "q1",
    "text": "\u7B49\u5019\u6211\u8D77\u4F86\u6307\u63A7\u7684\u65E5\u5B50\u3002",
    "style": "q1",
    "text": "\u6211\u5DF2\u6C7A\u5B9A\u53EC\u96C6\u5217\u570B\uFF0C\u532F\u805A\u5217\u90A6\uFF0C",
    "style": "q1",
    "text": "\u5411\u4ED6\u5011\u50BE\u5012\u6211\u7684\u70C8\u6012\uFF0C",
    "style": "q1",
    "text": "\u6211\u7684\u6012\u706B\u5FC5\u541E\u566C\u5929\u4E0B\u3002",
    "style": "q1",
    "text": "\u9019\u662F\u8036\u548C\u83EF\u8AAA\u7684\u3002",
    "style": "s1",
    "text": "\u8036\u548C\u83EF\u7684\u62EF\u6551",
    "style": "q1",
    "text": "\n    ",
    "v": "9",
    "text": "\u300C\u90A3\u6642\uFF0C\u6211\u5FC5\u6F54\u6DE8\u842C\u6C11\u7684\u53E3\uFF0C",
    "style": "q1",
    "text": "\u4ED6\u5011\u597D\u6C42\u544A\u6211\u7684\u540D\uFF0C",
    "style": "q1",
    "text": "\u540C\u5FC3\u5408\u610F\u5730\u4E8B\u5949\u6211\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "10",
    "text": "\u90A3\u4E9B\u656C\u62DC\u6211\u7684\uFF0C",
    "style": "q1",
    "text": "\u5C31\u662F\u6211\u90A3\u4E9B\u5206\u6563\u7684\u5B50\u6C11\u5FC5\u5F9E",
    "pn": "\u53E4\u5BE6",
    "text": "\u53E4\u5BE6",
    "text": "\u6CB3\u5C0D\u5CB8\u5E36\u8457\u4F9B\u7269\u4F86\u737B\u7D66\u6211\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "11",
    "text": "\u5230\u90A3\u5929\uFF0C\u4F60\u5FC5\u4E0D\u518D\u56E0\u80CC\u53DB\u6211\u800C\u7F9E\u6127\uFF0C",
    "style": "q1",
    "text": "\u56E0\u70BA\u90A3\u6642\u6211\u5FC5\u9664\u6389\u4F60\u90A3\u88E1\u7684\u72C2\u50B2\u4E4B\u5F92\uFF0C",
    "style": "q1",
    "text": "\u4F60\u5728\u6211\u7684\u8056\u5C71\u4E0A\u5FC5\u4E0D\u518D\u9AD8\u50B2\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "12",
    "text": "\u6211\u5FC5\u628A\u8B19\u5351\u8CA7\u82E6\u7684\u4EBA\u7559\u5728\u4F60\u90A3\u88E1\uFF0C",
    "style": "q1",
    "text": "\u4ED6\u5011\u5FC5\u6295\u9760\u5728\u8036\u548C\u83EF\u7684\u540D\u4E0B\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "13",
    "text": "\n    ",
    "pn": "\u4EE5\u8272\u5217",
    "text": "\u4EE5\u8272\u5217",
    "text": "\u7684\u9918\u6C11\u5FC5\u4E0D\u518D\u4F5C\u60E1\uFF0C",
    "style": "q1",
    "text": "\u4E0D\u518D\u6492\u8B0A\uFF0C\u53E3\u4E2D\u4E0D\u518D\u51FA\u8A6D\u8A50\u4E4B\u8A00\u3002",
    "style": "q1",
    "text": "\u4ED6\u5011\u5403\u559D\u8EBA\u81E5\uFF0C\u7121\u4EBA\u9A5A\u64FE\u3002\u300D",
    "style": "s1",
    "text": "\u6B61\u6A02\u4E4B\u6B4C",
    "style": "q1",
    "text": "\n    ",
    "v": "14",
    "text": "\u5C11\u5973",
    "pn": "\u932B\u5B89",
    "text": "\u932B\u5B89",
    "text": "\u554A\uFF0C\u6B4C\u5531\u5427\uFF01",
    "style": "q1",
    "text": "\n    ",
    "pn": "\u4EE5\u8272\u5217",
    "text": "\u4EE5\u8272\u5217",
    "text": "\u554A\uFF0C\u6B61\u547C\u5427\uFF01",
    "style": "q1",
    "text": "\u5C11\u5973",
    "pn": "\u8036\u8DEF\u6492\u51B7",
    "text": "\u8036\u8DEF\u6492\u51B7",
    "text": "\u554A\uFF0C\u76E1\u60C5\u5730\u6B61\u559C\u5FEB\u6A02\u5427\uFF01",
    "style": "q1",
    "text": "\n    ",
    "v": "15",
    "text": "\u8036\u548C\u83EF\u5DF2\u7D93\u64A4\u92B7\u5C0D\u4F60\u7684\u5211\u7F70\uFF0C",
    "style": "q1",
    "text": "\u8D95\u8D70\u4F60\u7684\u4EC7\u6575\uFF0C",
    "style": "q1",
    "text": "\n    ",
    "pn": "\u4EE5\u8272\u5217",
    "text": "\u4EE5\u8272\u5217",
    "text": "\u7684\u738B\u8036\u548C\u83EF\u8207\u4F60\u540C\u5728\uFF0C",
    "style": "q1",
    "text": "\u4F60\u5FC5\u4E0D\u518D\u61FC\u6015\u707D\u798D\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "16",
    "text": "\u5230\u90A3\u5929\uFF0C",
    "pn": "\u8036\u8DEF\u6492\u51B7",
    "text": "\u8036\u8DEF\u6492\u51B7",
    "text": "\u5FC5\u807D\u5230\u9019\u6A23\u7684\u8A71\uFF1A",
    "style": "q1",
    "text": "\u300C",
    "pn": "\u932B\u5B89",
    "text": "\u932B\u5B89",
    "text": "\u554A\uFF0C\u4E0D\u8981\u5BB3\u6015\uFF01\u4E0D\u8981\u96D9\u624B\u767C\u8EDF\uFF01",
    "style": "q1",
    "text": "\n    ",
    "v": "17",
    "text": "\u4F60\u7684\u4E0A\u5E1D\u8036\u548C\u83EF\u8207\u4F60\u540C\u5728\uFF0C",
    "style": "q1",
    "text": "\u7942\u662F\u5927\u80FD\u7684\u62EF\u6551\u8005\uFF0C",
    "style": "q1",
    "text": "\u7942\u5FC5\u56E0\u4F60\u800C\u6B61\u559C\u5FEB\u6A02\uFF0C\u6B61\u7136\u6B4C\u5531\uFF0C",
    "style": "q1",
    "text": "\u7528\u7942\u7684\u611B\u5B89\u6170\u4F60\u3002\u300D",
    "style": "b",
    "style": "q1",
    "text": "\n    ",
    "v": "18",
    "text": "\u300C\u6211\u5FC5\u62DB\u805A\u4F60\u90A3\u88E1\u56E0\u4E0D\u80FD\u5B88\u7BC0\u671F\u800C\u6182\u6101\u7684\u4EBA\uFF0C",
    "style": "q1",
    "text": "\u4F7F\u4F60\u4E0D\u518D\u8499\u53D7\u7F9E\u8FB1\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "19",
    "text": "\u770B\u554A\uFF0C\u90A3\u6642\u6211\u5FC5\u61F2\u7F70\u6240\u6709\u6B3A\u58D3\u4F60\u7684\u4EBA\uFF0C",
    "style": "q1",
    "text": "\u62EF\u6551\u7638\u817F\u7684\uFF0C\u53EC\u56DE\u5206\u6563\u7684\uFF0C",
    "style": "q1",
    "text": "\u4F7F\u4ED6\u5011\u7684\u7F9E\u8FB1\u5316\u70BA\u5C0A\u69AE\uFF0C\u8072\u540D\u9060\u63DA\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "20",
    "text": "\u90A3\u6642\uFF0C\u6211\u5FC5\u62DB\u805A\u4F60\u5011\uFF0C\u5E36\u4F60\u5011\u56DE\u5230\u6545\u571F\u3002",
    "style": "q1",
    "text": "\u4F60\u5011\u89AA\u773C\u76EE\u7779\u6211\u5E36\u4F60\u5011\u56DE\u5230\u6545\u571F\u5F8C\uFF0C",
    "style": "q1",
    "text": "\u6211\u5FC5\u4F7F\u4F60\u5011\u5728\u5929\u4E0B\u842C\u6C11\u4E2D\u8072\u540D\u9060\u63DA\uFF0C\u5F97\u4EAB\u5C0A\u69AE\u3002\u300D",
    "style": "q1",
    "text": "\u9019\u662F\u8036\u548C\u83EF\u8AAA\u7684\u3002"
  ]
}