[["c","20"],["v","1"],["","酒使人放肆，烈酒叫人吵鬧。"],["q2","因酒誤事，實為不智。"],["v","2"],["","君王震怒如獅子咆哮，"],["q2","觸怒他的人自尋死路。"],["v","3"],["","避免紛爭是光榮，"],["q2","愚人個個愛爭吵。"],["v","4"],["","懶惰人不按時耕種，"],["q2","收穫時必一無所得。"],["v","5"],["","人心的謀略如深水，"],["q2","唯有哲士才能汲取。"],["v","6"],["","人都聲稱自己忠誠，"],["q2","但誰能找到忠信者？"],["v","7"],["","義人行為正直，"],["q2","他的後代也蒙福。"],["v","8"],["","王坐在寶座上審判，"],["q2","他的眼目明察罪惡。"],["v","9"],["","誰敢說：「我心靈潔淨，純潔無罪？」"],["v","10"],["","缺斤少兩、尺寸不足，"],["q2","都為耶和華所憎惡。"],["v","11"],["","孩童的舉動是否純良，"],["q2","顯明他的本性。"],["v","12"],["","能聽的耳，能看的眼，"],["q2","兩者都為耶和華所造。"],["v","13"],["","不要貪睡，免得貧窮；"],["q2","勤勞工作，糧食豐足。"],["v","14"],["","買主總說貨物不好，"],["q2","買走後又連連自誇。"],["v","15"],["","世間有黃金和許多寶石，"],["q2","唯有博學之言才是珍寶。"],["v","16"],["","誰為陌生人作保，就拿去誰的外衣；"],["q2","誰為外族人作保，誰就要承擔責任。"],["v","17"],["","騙來的餅覺得香甜，"],["q2","最後卻是滿口泥沙。"],["v","18"],["","集思出良策，"],["q2","征戰靠智謀。"],["v","19"],["","嚼舌的人洩露秘密，"],["q2","不可結交多嘴的人。"],["v","20"],["","咒罵父母的人，"],["q2","他的生命"],["","必消逝在黑暗中。"],["v","21"],["","起初輕易得來之財，"],["q2","最終不會成為祝福。"],["v","22"],["","不要說：「我要以惡報惡」，"],["q2","要等候耶和華，祂必替你伸冤。"],["v","23"],["","耶和華憎惡缺斤少兩，"],["q2","祂厭惡騙人的天平。"],["v","24"],["","人的腳步由耶和華所定，"],["q2","人怎能知道自己的道路？"],["v","25"],["","人向主許願要三思，"],["q2","免得事後作繭自縛。"],["v","26"],["","明君必篩出惡人，"],["q2","用石磙碾軋他們。"],["v","27"],["","人的靈是耶和華的燈，"],["q2","鑒察人的肺腑心腸。"],["v","28"],["","君王靠慈愛和忠信護佑，"],["q2","他的王位因慈愛而穩固。"],["v","29"],["","力量是青年的榮耀，"],["q2","白髮是老人的尊榮。"],["v","30"],["","鞭傷能驅除罪惡，"],["q2","責打能潔淨人心。"]]