    "v": "9",
    "text": "\n    ",
    "pn": "\u53E4\u5BE6",
    "text": "\u53E4\u5BE6",
    "text": "\u548C",
    "pn": "\u57C3\u53CA",
    "text": "\u57C3\u53CA",
    "text": "\u662F\u5979\u7121\u7AAE\u7684\u529B\u91CF\uFF0C",
    "style": "q1",
    "text": "\n    ",
    "pn": "\u5F17",
    "text": "\u5F17",
    "text": "\u4EBA\u548C",
    "pn": "\u5229\u6BD4\u4E9E",
    "text": "\u5229\u6BD4\u4E9E",
    "text": "\u4EBA\u662F\u5979\u7684\u5E6B\u624B\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "10",
    "text": "\u53EF\u662F\uFF0C\u5979\u537B\u88AB\u64C4\u53BB\uFF0C\u6D41\u843D\u4ED6\u9109\uFF0C",
    "style": "q1",
    "text": "\u5979\u7684\u5B30\u5B69\u88AB\u6454\u6B7B\u5728\u5404\u500B\u8857\u982D\u3002",
    "style": "q1",
    "text": "\u5979\u7684\u5B98\u9577\u88AB\u62BD\u7C64\u74DC\u5206\uFF0C",
    "style": "q1",
    "text": "\u5979\u6240\u6709\u7684\u9996\u9818\u90FD\u88AB\u9396\u93C8\u9396\u8457\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "11",
    "text": "\n    ",
    "pn": "\u5C3C\u5C3C\u5FAE",
    "text": "\u5C3C\u5C3C\u5FAE",
    "text": "\u554A\uFF0C",
    "style": "q1",
    "text": "\u4F60\u4E5F\u5FC5\u50CF\u9189\u6F22\u4E00\u6A23\uFF0C",
    "style": "q1",
    "text": "\u4F60\u5FC5\u5C0B\u627E\u85CF\u8EAB\u4E4B\u8655\u8EB2\u907F\u6575\u4EBA\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "12",
    "text": "\u4F60\u6240\u6709\u7684\u5821\u58D8\u90FD\u50CF\u7121\u82B1\u679C\u6A39\u4E0A\u7684\u521D\u719F\u679C\u5B50\uFF0C",
    "style": "q1",
    "text": "\u4E00\u6416\u6A39\uFF0C\u679C\u5B50\u5C31\u6389\u9032\u5403\u8005\u53E3\u4E2D\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "13",
    "text": "\u770B\u554A\uFF0C",
    "style": "q1",
    "text": "\u4F60\u7684\u8ECD\u968A\u5C31\u50CF\u67D4\u5F31\u7684\u5973\u4EBA\uFF0C",
    "style": "q1",
    "text": "\u4F60\u5883\u5167\u7684\u95DC\u53E3\u5411\u6575\u4EBA\u655E\u958B\uFF0C",
    "style": "q1",
    "text": "\u4F60\u7684\u9580\u9582\u88AB\u706B\u71D2\u6BC0\u3002",
    "style": "b",
    "style": "q1",
    "text": "\n    ",
    "v": "14",
    "text": "\u4F60\u8981\u84C4\u6C34\u4EE5\u9632\u570D\u56F0\uFF0C",
    "style": "q1",
    "text": "\u8981\u52A0\u5F37\u9632\u79A6\uFF01",
    "style": "q1",
    "text": "\u8981\u8D70\u9032\u571F\u5751\u8E29\u6CE5\uFF0C",
    "style": "q1",
    "text": "\u62FF\u8D77\u78DA\u6A21\u9020\u78DA\uFF01",
    "style": "q1",
    "text": "\n    ",
    "v": "15",
    "text": "\u4F46\u5728\u90A3\u88E1\uFF0C\u706B\u8981\u541E\u566C\u4F60\uFF0C",
    "style": "q1",
    "text": "\u5200\u528D\u8981\u6BBA\u622E\u4F60\uFF0C",
    "style": "q1",
    "text": "\u8981\u50CF\u8757\u87F2\u541E\u6C92\u4F60\u3002",
    "style": "q1",
    "text": "\u4F60\u53EA\u7BA1\u50CF\u8757\u87F2\u4E00\u6A23\u7E41\u6B96\uFF0C",
    "style": "q1",
    "text": "\u5982\u86B1\u8722\u4E00\u822C\u589E\u591A\u5427\uFF01",
    "style": "q1",
    "text": "\n    ",
    "v": "16",
    "text": "\u4F60\u7684\u5546\u8CC8\u591A\u904E\u5929\u4E0A\u7684\u7E41\u661F\uFF0C",
    "style": "q1",
    "text": "\u4F46\u4ED6\u5011\u50CF\u5403\u5149\u7530\u5730\u5F8C\u98DB\u53BB\u7684\u8757\u87F2\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "17",
    "text": "\u4F60\u7684\u81E3\u50D5\u50CF\u8757\u87F2\uFF0C",
    "style": "q1",
    "text": "\u4F60\u7684\u5C07\u9818\u50CF\u6210\u7FA4\u7684\u86B1\u8722\u3002",
    "style": "q1",
    "text": "\u7260\u5011\u5BD2\u65E5\u805A\u96C6\u5728\u7246\u4E0A\uFF0C",
    "style": "q1",
    "text": "\u592A\u967D\u4E00\u51FA\u4F86\u5C31\u98DB\u8D70\u4E86\uFF0C",
    "style": "q1",
    "text": "\u7121\u4EBA\u77E5\u9053\u7260\u5011\u7684\u884C\u8E64\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "18",
    "text": "\n    ",
    "pn": "\u4E9E\u8FF0",
    "text": "\u4E9E\u8FF0",
    "text": "\u738B\u554A\uFF01",
    "style": "q1",
    "text": "\u4F60\u7684\u7267\u4EBA\u6C89\u7761\uFF0C\u8CB4\u65CF\u9163\u7720\uFF0C",
    "style": "q1",
    "text": "\u4F60\u7684\u767E\u59D3\u5206\u6563\u5728\u7FA4\u5C71\u4E0A\uFF0C",
    "style": "q1",
    "text": "\u7121\u4EBA\u62DB\u805A\u4ED6\u5011\u3002",
    "style": "q1",
    "text": "\n    ",
    "v": "19",
    "text": "\u4F60\u7684\u5275\u50B7\u7121\u6CD5\u6551\u6CBB\uFF0C",
    "style": "q1",
    "text": "\u4F60\u7684\u50B7\u52E2\u6975\u5176\u56B4\u91CD\u3002",
    "style": "q1",
    "text": "\u807D\u898B\u9019\u6D88\u606F\u7684\u4EBA\u5FC5\u9F13\u638C\u6B61\u547C\uFF0C",
    "style": "q1",
    "text": "\u56E0\u70BA\u8AB0\u6C92\u53D7\u904E\u4F60\u7121\u4F11\u6B62\u7684\u66B4\u8650\u5462\uFF1F"
  ]
}